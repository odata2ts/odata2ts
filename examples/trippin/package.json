{
  "name": "@odata2ts/example-trippin",
  "version": "0.0.0",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "description": "Example using the publicly available Trippin service",
  "license": "MIT",
  "repository": "git@github.com:odata2ts/odata2ts.git",

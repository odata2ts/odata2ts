{
  "name": "@odata2ts/example-bookshop",
  "version": "0.9.3",
  "private": true,
  "description": "Example using CAP",
  "repository": "git@github.com:odata2ts/odata2ts.git",
  "license": "MIT",
  "author": "texttechne",
  "files": [
    "app",
    "db",
    "server/index.cds",
    "server/index.js",
    "srv"
  ],
  "scripts": {
    "build": "yarn clean",
    "clean": "rimraf build src",
    "compile": "tsc",
    "gen-meta": "cds server/srv/cat-service.cds -2 edmx > resource/catalog-srv.xml && cds server/srv/admin-service.cds -2 edmx > resource/admin-srv.xml ",
    "generate-from-url": "odata2ts && tsc -p tsconfig.compile.json",
    "int-test": "vitest run --dir int-test",
    "start": "cds run",
    "watch": "cds watch"
  },
  "dependencies": {
    "@odata2ts/converter-big-number": "^0.2.7",
    "@odata2ts/http-client-axios": "^0.12.0",
    "@odata2ts/odata-service": "^0.23.0",
    "@sap/cds": "7.9.4",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "axios": "0.27.2",
    "bignumber.js": "^9.1.1",
    "express": "^4.18.2",
    "passport": ">=0.6.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@odata2ts/odata2ts": "^0.39.1",
    "sqlite3": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "readme": "README.md"

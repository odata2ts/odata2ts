{
  "name": "@odata2ts/example-bookshop",
  "version": "0.4.1",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "description": "Example using CAP",
  "license": "MIT",
  "repository": "git@github.com:odata2ts/odata2ts.git",
  "author": "texttechne",
  "scripts": {
    "build": "yarn clean && yarn generate",
    "clean": "rimraf build src",
    "compile": "tsc",
    "gen-meta": "cds server/srv/cat-service.cds -2 edmx > resource/catalog-srv.xml && cds server/srv/admin-service.cds -2 edmx > resource/admin-srv.xml ",
    "generate": "odata2ts",
    "int-test": "jest ./int-test",
    "start": "cds run",
    "type-test": "tsc",
    "watch": "cds watch"
  },
  "files": [
    "app",
    "db",
    "server/index.cds",
    "server/index.js",
    "srv"
  ],
  "dependencies": {
    "@odata2ts/http-client-axios": "^0.8.0",
    "@odata2ts/odata-service": "^0.15.2",
    "@sap/cds": ">=5.9",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "axios": "0.27.2",
    "express": "^4.18.2",
    "passport": ">=0.6.0"
  },
  "devDependencies": {
    "@odata2ts/odata2ts": "^0.24.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.1",
    "sqlite3": "^5",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "readme": "README.md"
}

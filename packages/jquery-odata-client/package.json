  "dependencies": {
    "@odata2ts/odata-client-api": "^0.6.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/jquery": "^3.5.16",
    "@types/node": "^17.0.23",
    "jest": "27.5.1",
    "jquery": "^3.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.9.4"
  },
  "types": "./lib/index.d.ts"

{
  "name": "@odata2ts/odata2model",
  "version": "0.8.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Generates TypeScript interfaces & Query Objects from a given OData service",
  "license": "MIT",
  "repository": "git@github.com:odata2ts/odata2ts.git",
  "author": "texttechne",
  "bin": "lib/run-cli.js",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf lib build",
    "compile": "tsc",
    "generate": "ts-node --cwd-mode src/run-cli.ts ",
    "dev": "yarn clean && yarn dev-nw && yarn dev-trippin",
    "dev-nw": "yarn generate -o build/northwind -s int-test/fixture/northwind-v4.xml",
    "dev-trippin": "yarn generate -o build/trippin -s int-test/fixture/trippin.xml",
    "int-test": "yarn clean && yarn dev-trippin && jest ./int-test",
    "test": "jest ./test"
  },
  "files": [
    "*.md",
    "bin",
    "lib",
    "LICENSE"
  ],
  "peerDependencies": {
    "@odata2ts/odata-client-api": "*",
    "@odata2ts/odata-query-objects": "*",
    "@odata2ts/odata-service": "*"
  },
  "dependencies": {
    "commander": "^9.1.0",
    "cosmiconfig": "^7.0.1",
    "fs-extra": "^10.0.1",
    "prettier": "^2.6.2",
    "ts-morph": "^14.0.0",
    "upper-case-first": "^2.0.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@odata2ts/odata-client-api": "^0.2.0",
    "@odata2ts/odata-query-objects": "^0.9.0",
    "@odata2ts/odata-service": "^0.4.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.4",
    "@types/xml2js": "^0.4.9",
    "axios": "^0.26.1",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "gitHead": "b88f1a6e7ce84d26d1d7efa5a61dfc66a4626ec3",
  "readme": "README.md"
}

{
  "name": "@odata2ts/fetch-odata-client",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "HTTP client based on fetch and consumable by odata2ts",
  "license": "MIT",
  "repository": "git@github.com:odata2ts/odata2ts.git",
